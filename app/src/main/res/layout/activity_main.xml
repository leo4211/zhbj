    android:layout_height="match_parent"
    tools:context="test.liuyang.com.zhbj.MainActivity"
    >

   <test.liuyang.com.zhbj.customview.SlidingMenu
       android:layout_width="match_parent"
       android:layout_height="match_parent">

      <LinearLayout
          android:layout_width="match_parent"
          android:layout_height="match_parent"
